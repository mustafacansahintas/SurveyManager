@model AnketYonetimSistemi.Models.DashboardModel

@{ 

    double sayi = ((@Model.CompletePerCount*100) / @Model.TotalPerCount);

    double yüzde = Math.Round(sayi,0);
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Mosaddek">
    <meta name="keyword" content="FlatLab, Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <link rel="shortcut icon" href="http://thevectorlab.net/flatlab/img/favicon.png">

    <title>FlatLab - Flat & Responsive Bootstrap Admin Template</title>

    <!-- Bootstrap core CSS -->
    <link href="~/Dashboard/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Dashboard/css/bootstrap-reset.css" rel="stylesheet">
    <!--external css-->
    <link href="~/Dashboard/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Dashboard/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.css" rel="stylesheet" type="text/css" media="screen" />
    <link rel="stylesheet" href="~/Dashboard/css/owl.carousel.css" type="text/css">

    <!--right slidebar-->
    <link href="~/Dashboard/css/slidebars.css" rel="stylesheet">

    <!-- Custom styles for this template -->

    <link href="~/Dashboard/css/style.css" rel="stylesheet">
    <link href="~/Dashboard/css/style-responsive.css" rel="stylesheet" />


    <!-- HTML5 shim and Respond.js IE8 support of HTML5 tooltipss and media queries -->
    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
      <script src="js/respond.min.js"></script>
    <![endif]-->
</head>

<body>
    <section id="container">


        <section style="margin:auto"  id="main-content">
            <section class="wrapper">
                <!--state overview start-->
                <div class="row state-overview">
                    <div class="col-lg-3 col-sm-6">
                        <section data-id=@Model.TotalPerCount id="dashboardpanel1" class="panel">
                            <div class="symbol terques">
                                <i class="fa fa-user"></i>
                            </div>
                            <div class="value">
                                <h1 class="count">
                                    0
                                </h1>
                                <p>Toplam Katılımcı Sayısı</p>
                            </div>
                        </section>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <section data-id=@Model.CompletePerCount id="dashboardpanel2" class="panel">
                            <div class="symbol red">
                                <i class="fa fa-tags"></i>
                            </div>
                            <div class="value">
                                <h1 class=" count2">
                                    0
                                </h1>
                                <p>Anketi Tamamlayanların Sayısı</p>
                            </div>
                        </section>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <section data-id=@Model.TotalResponseCount id="dashboardpanel3" class="panel">
                            <div class="symbol yellow">
                                <i class="fa fa-shopping-cart"></i>
                            </div>
                            <div class="value">
                                <h1 class=" count3">
                                    0
                                </h1>
                                <p>Toplam Seçenek Sayısı</p>
                            </div>
                        </section>
                    </div>
                    <div class="col-lg-3 col-sm-6">
                        <section data-id=@Model.MarkedResponseCount id="dashboardpanel4" class="panel">
                            <div class="symbol blue">
                                <i class="fa fa-bar-chart-o"></i>
                            </div>
                            <div class="value">
                                <h1 class=" count4">
                                    0
                                </h1>
                                <p>Yanıtlanan Seçenek Sayısı</p>
                            </div>
                        </section>
                    </div>
                </div>
                <!--state overview end-->
                <div class="col-lg-12">
                    <!--revenue start-->
                    <section class="panel">
                        <div class="revenue-head">
                            <span>
                                <i class="fa fa-bar-chart-o"></i>
                            </span>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-6 col-sm-6 text-center">
                                    <div class="easy-pie-chart">
                                        <div class="percentage" data-percent=@yüzde><span>@yüzde</span>%</div>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-sm-6">
                                    <div class="chart-info chart-position">
                                        <span class="increase"></span>
                                        <span>Ankete Katılım Yüzdesi</span>
                                    </div>
                                    <div class="chart-info">
                                        <span class="decrease"></span>
                                        <span>Ankete Katılmayan Yüzdesi</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
              
                </div>

            </section>
        </section>


    </section>
    <!-- js placed at the end of the document so the pages load faster -->
    <script src="~/Dashboard/js/jquery.js"></script>
    <script src="~/Dashboard/js/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="~/Dashboard/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/Dashboard/js/jquery.scrollTo.min.js"></script>
    <script src="~/Dashboard/js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="~/Dashboard/js/jquery.sparkline.js" type="text/javascript"></script>
    <script src="~/Dashboard/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="~/Dashboard/js/owl.carousel.js"></script>
    <script src="~/Dashboard/js/jquery.customSelect.min.js"></script>
    <script src="~/Dashboard/js/respond.min.js"></script>

    <!--right slidebar-->
    <script src="~/Dashboard/js/slidebars.min.js"></script>

    <!--common script for all pages-->
    <script src="~/Dashboard/js/common-scripts.js"></script>

    <!--script for this page-->
    <script src="~/Dashboard/js/sparkline-chart.js"></script>
    <script src="~/Dashboard/js/easy-pie-chart.js"></script>
    <script src="~/Dashboard/js/count.js"></script>

    <script>//owl carousel

      $(document).ready(function() {
          $("#owl-demo").owlCarousel({
              navigation : true,
              slideSpeed : 300,
              paginationSpeed : 400,
              singleItem : true,
			  autoPlay:true

          });
      });

      //custom select box

      $(function(){
          $('select.styled').customSelect();
      });

      $(window).on("resize",function(){
          var owl = $("#owl-demo").data("owlCarousel");
          owl.reinit();
      });</script>

</body>
</html>
